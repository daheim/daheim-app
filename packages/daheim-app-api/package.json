{
  "name": "daheim-app-api",
  "description": "API Server of Daheim",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node lib/app",
    "dev": "better-npm-run dev",
    "check": "eslint .",
    "build": "babel src/ -d lib/ --copy-files",
    "clean": "rm -rf lib"
  },
