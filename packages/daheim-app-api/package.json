  "repository": {
    "type": "git",
    "url": "https://github.com/egergo/daheim-app-api.git"
  },
  "engines": {
    "node": "5.11.0",
    "npm": "3.8.8"
  },
  "dependencies": {
    "axios": "^0.13.1",
    "azure-common": "^0.9.16",
    "azure-storage": "^0.6.0",
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "bcryptjs": "^2.3.0",
    "bluebird": "^3.0.5",
    "body-parser": "^1.13.3",
    "convict": "^1.0.1",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "es6-error": "^2.0.2",
    "express": "^4.13.1",
    "gravatar": "^1.5.2",
    "intl-format-cache": "^2.0.5",
    "intl-messageformat": "^1.3.0",
